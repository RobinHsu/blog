{
  "extends": "tslint:recommended",
  "rules": {
    "no-console": false,
    "quotemark": [true, "single", "jsx-double"],
    "ban-types": [false],
    "curly": [false],
    "interface-name": [false],
    "object-literal-sort-keys": [false],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never"
        }
      }
    ]
  }
}