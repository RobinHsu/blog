{
  "name": "blog",
  "version": "0.0.1",
  "description": "My blog",
  "main": "index.js",
  "scripts": {
    "dev": "npm run serve-build && npm run watch",
    "serve-build": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Webpack\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\" \"npm run webpack-dev\"",
    "watch-ts": "tsc -w",
    "serve": "PORT=7001 NODE_ENV=development nodemon dist/server.js",
    "webpack-dev": "webpack --progress --config bin/webpack.dev.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinHsu/blog.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobinHsu/blog/issues"
  },
  "homepage": "https://github.com/RobinHsu/blog#readme",
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/koa": "^2.0.44",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/react-router-dom": "^4.2.4",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "lodash": "^4.17.5",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.1",
    "postcss-loader": "^2.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^3.1.0",
    "koa": "^2.5.0",
    "koa-send": "^4.1.3",
    "log4js": "^2.5.3",
    "moment": "^2.20.1"
  }
}
